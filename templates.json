[
  {
    "type": "container",
    "title": "SABnzbd",
    "name": "SABnzbd",
    "categories": ["Downloading"],
    "description": "SABnzbd makes Usenet as simple and streamlined as possible by automating everything we can. All you have to do is add an .nzb. SABnzbd takes over from there, where it will be automatically downloaded, verified, repaired, extracted and filed away with zero human interaction.",
    "categories": ["docker"],
    "platform": "linux",
    "logo": "https://raw.githubusercontent.com/sabnzbd/sabnzbd/master/interfaces/Config/templates/staticcfg/ico/apple-touch-icon-180x180-precomposed.png",
    "image": "linuxserver/sabnzbd:latest",
    "privileged": false,
     "interactive": false,
  "restart_policy": "always",
    "hostname": "always",
  "network": "bridge",
    "ports": [
      "8080/tcp", "9090/tcp"
    ],
    "volumes": [
    {
      "container": "/downloads",
       "bind": "/mnt/downloads"
    },
    {
      "container": "/config",
      "bind": "/mnt/appdata/sabnzbd"
    }
      ],
    "env": [
    {
      "name": "PGID",
      "label": "GroupID",
      "description": "GroupID",
      "set": "1000"
    },
    {
      "name": "PUID",
      "label": "UserID",
      "description": "UserID",
      "set": "1000"
    },
    {
      "name": "TZ",
      "label": "Timezone",
      "description": "Setting timezone information, ex: 'America/New_York'.",
      "set": "America/New_York"
    }
      ],
    "note": "Github: <a href=\"https://github.com/linuxserver/docker-sabnzbd\" rel=\"nofollow\">linuxserver/docker-sabnzbd</a><br/>Docker Hub: <a href=\"https://hub.docker.com/r/linuxserver/sabnzbd/\" rel=\"nofollow\">linuxserver/docker-sabnzbd</a>"
  }
]
